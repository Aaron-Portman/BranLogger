<% workouts.forEach(workout => { %>
    <div style="background-color: <%= workout.gradeColor %>">
        Workouts:

        <%=workout.workout%>
        <%=workout.year%>
        <%=workout.month%>
        <%=workout.day%>
        <%=workout.dayOfWeek%>
        <%=workout.grade%>
        <%=workout.notes%>

    </div>
<% }); %>

<% easyruns.forEach(run => { %>
    <div style="background-color: <%= run.gradeColor %>">
        <h1>Easy Runs:</h1>
        <h3>Distance: <%=run.mileage%> miles</h3>
        <h3> Time: <%=run.minutes%>:<%=run.seconds%> </h3>
        <h3>Pace: <%= run.avgTime %></h3>
        <h3>Comments: <%= run.notes %></h3>
        <!-- <%=run.year%>
        <%=run.month%>
        <%=run.day%>
        <%=run.dayOfWeek%>
        <%=run.grade%>
        <%=run.notes%> -->

    </div>
<% }); %>